{
  "name": "fileinfo",
  "displayName": "FileInfo",
  "description": "Show the file info in the statusbar",
  "version": "0.1.0",
  "publisher": "FastChen",
  "icon": "images/vscode-filesize_512.png",
  "homepage": "https://github.com/FastChen/vscode-fileinfo/blob/master/README.md",
  "bugs": {
    "url": "https://github.com/FastChen/vscode-fileinfo/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FastChen/vscode-fileinfo.git"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": ["*"],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  },
  "contributes": {
    "commands": [],
    "configuration": {
      "title": "FileInfo",
      "properties": {
        "fileinfo.settings.statusbar.location": {
          "type": "string",
          "default": "right",
          "description": "StatusBar show location",
          "enum": ["left", "right"]
        }
      }
    }
  }
}